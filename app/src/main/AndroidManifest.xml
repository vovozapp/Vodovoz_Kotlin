<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vodovoz.app">

    <queries>
        <intent>
            <action android:name="android.intent.action.VIEW" />
            <category android:name="android.intent.category.BROWSABLE" />
            <data android:scheme="https" />
        </intent>
    </queries>


    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Vodovoz"
        android:name="com.vodovoz.app.ui.base.VodovozApplication"
        android:usesCleartextTraffic="true">
        <activity
            android:name="com.vodovoz.app.ui.base.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <!-- Catalog -->
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/catalog"
                    android:scheme="@string/new_app_ssl" />
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/catalog"
                    android:scheme="@string/new_app_ssl_not" />
                <!-- Action -->
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/action"
                    android:scheme="@string/new_app_ssl" />
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/action"
                    android:scheme="@string/new_app_ssl_not" />
                <!-- Brand -->
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/brand"
                    android:scheme="@string/new_app_ssl" />
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/brand"
                    android:scheme="@string/new_app_ssl_not" />

                <!-- mobile_app -->
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/mobile_app"
                    android:scheme="@string/new_app_ssl" />
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/mobile_app"
                    android:scheme="@string/new_app_ssl_not" />
                <!-- About -->
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/about"
                    android:scheme="@string/new_app_ssl" />
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/about"
                    android:scheme="@string/new_app_ssl_not" />

                <!-- dostavka -->
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/dostavka"
                    android:scheme="@string/new_app_ssl" />
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/dostavka"
                    android:scheme="@string/new_app_ssl_not" />
                <!-- service - услуги -->
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/service"
                    android:scheme="@string/new_app_ssl" />
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/service"
                    android:scheme="@string/new_app_ssl_not" />

                <!-- санитарная обработка -->
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/remont_kulerov"
                    android:scheme="@string/new_app_ssl" />
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/remont_kulerov"
                    android:scheme="@string/new_app_ssl_not" />

                <!-- feedback - связаться с нами -->
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/feedback"
                    android:scheme="@string/new_app_ssl" />
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/feedback"
                    android:scheme="@string/new_app_ssl_not" />
                <!-- Glavnee -->

                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/gl"
                    android:scheme="@string/new_app_ssl" />
                <data
                    android:host="@string/new_app"
                    android:pathPrefix="/gl"
                    android:scheme="@string/new_app_ssl_not" />
            </intent-filter>
        </activity>
    </application>

</manifest>