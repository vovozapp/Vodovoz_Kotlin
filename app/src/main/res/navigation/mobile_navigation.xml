<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.vodovoz.app.ui.components.fragment.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_main_home" >
        <action
            android:id="@+id/action_to_promotionDetailFragment"
            app:destination="@id/promotionDetailFragment" />
        <action
            android:id="@+id/action_to_allPromotionsFragment"
            app:destination="@id/allPromotionsFragment" />
        <action
            android:id="@+id/action_to_allBrandsFragment"
            app:destination="@id/allBrandsFragment"
            app:exitAnim="@anim/to_top" />
        <action
            android:id="@+id/action_to_productsCatalogFragment"
            app:destination="@id/productsCatalogFragment" />
        <action
            android:id="@+id/action_to_paginatedProductsCatalogWithoutFiltersFragment"
            app:destination="@id/paginatedProductsCatalogWithoutFiltersFragment" />
        <action
            android:id="@+id/action_to_productDetailFragment"
            app:destination="@id/productDetailFragment" />
        <action
            android:id="@+id/action_to_paginatedProductsCatalogFragment"
            app:destination="@id/paginatedProductsCatalogFragment" />
        <action
            android:id="@+id/action_to_popupNewsBottomFragment"
            app:destination="@id/popupNewsBottomFragment" />
        <action
            android:id="@+id/action_to_fullScreenHistorySliderFragment"
            app:destination="@id/fullScreenHistorySliderFragment" />
        <action
            android:id="@+id/action_to_aboutServicesDialogFragment"
            app:destination="@id/aboutServicesDialogFragment" />
        <action
            android:id="@+id/action_to_aboutAppDialogFragment"
            app:destination="@id/aboutAppDialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/catalogFragment"
        android:name="com.vodovoz.app.ui.components.fragment.catalog.CatalogFragment"
        android:label="fragment_catalog"
        tools:layout="@layout/fragment_main_catalog" >
        <action
            android:id="@+id/action_to_paginatedProductsCatalogFragment"
            app:destination="@id/paginatedProductsCatalogFragment" />
    </fragment>
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.vodovoz.app.ui.components.fragment.cart.CartFragment"
        android:label="fragment_cart"
        tools:layout="@layout/fragment_main_cart" >
        <action
            android:id="@+id/action_to_giftsBottomFragment"
            app:destination="@id/giftsBottomFragment" />
        <action
            android:id="@+id/action_to_productDetailFragment"
            app:destination="@id/productDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/paginatedProductsCatalogFragment"
        android:name="com.vodovoz.app.ui.components.fragment.paginated_products_catalog.PaginatedProductsCatalogFragment"
        android:label="PaginatedProductsCatalogFragment">
        <argument
            android:name="categoryId"
            app:argType="long" />
        <action
            android:id="@+id/action_to_productDetailFragment"
            app:destination="@id/productDetailFragment" />
        <action
            android:id="@+id/action_to_singleRootCatalogBottomFragment"
            app:destination="@id/singleRootCatalogBottomFragment" />
        <action
            android:id="@+id/action_to_sortProductsSettingsBottomFragment"
            app:destination="@id/sortProductsSettingsBottomFragment" />
        <action
            android:id="@+id/action_to_productFiltersFragment"
            app:destination="@id/productFiltersFragment" />
    </fragment>
    <dialog
        android:id="@+id/sortProductsSettingsBottomFragment"
        android:name="com.vodovoz.app.ui.components.fragment.sort_products.SortProductsSettingsBottomFragment"
        android:label="SortProductsSettingsBottomFragment">
        <argument
            android:name="sortType"
            app:argType="string" />
    </dialog>
    <fragment
        android:id="@+id/productDetailFragment"
        android:name="com.vodovoz.app.ui.components.fragment.product_detail.ProductDetailFragment"
        android:label="ProductDetailFragment" >
        <argument
            android:name="productId"
            app:argType="long" />
        <action
            android:id="@+id/action_to_self"
            app:destination="@id/productDetailFragment" />
        <action
            android:id="@+id/action_to_promotionDetailFragment"
            app:destination="@id/promotionDetailFragment" />
        <action
            android:id="@+id/action_to_paginatedProductsCatalogFragment"
            app:destination="@id/paginatedProductsCatalogFragment" />
        <action
            android:id="@+id/action_to_paginatedProductsCatalogWithoutFiltersFragment"
            app:destination="@id/paginatedProductsCatalogWithoutFiltersFragment" />
        <action
            android:id="@+id/action_to_fullScreenDetailPicturesSliderFragment"
            app:destination="@id/fullScreenDetailPicturesSliderFragment" />
        <action
            android:id="@+id/action_to_youTubeVideoFragmentDialog"
            app:destination="@id/youTubeVideoFragmentDialog" />
        <action
            android:id="@+id/action_to_allCommentsByProductDialogFragment"
            app:destination="@id/allCommentsByProductDialogFragment" />
        <action
            android:id="@+id/action_to_sendCommentAboutProductFragment"
            app:destination="@id/sendCommentAboutProductFragment" />
    </fragment>
    <fragment
        android:id="@+id/promotionDetailFragment"
        android:name="com.vodovoz.app.ui.components.fragment.promotion_detail.PromotionDetailFragment"
        android:label="PromotionDetailFragment" >
        <argument
            android:name="promotionId"
            app:argType="long" />
        <action
            android:id="@+id/action_to_productDetailFragment"
            app:destination="@id/productDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/allPromotionsFragment"
        android:name="com.vodovoz.app.ui.components.fragment.all_promotions.AllPromotionsFragment"
        android:label="AllPromotionsFragment" >
        <argument
            android:name="dataSource"
            app:argType="com.vodovoz.app.ui.components.fragment.all_promotions.AllPromotionsFragment$DataSource" />
        <action
            android:id="@+id/action_to_promotionFiltersBottomFragment"
            app:destination="@id/promotionFiltersBottomFragment" />
        <action
            android:id="@+id/action_to_promotionDetailFragment"
            app:destination="@id/promotionDetailFragment" />
    </fragment>
    <dialog
        android:id="@+id/promotionFiltersBottomFragment"
        android:name="com.vodovoz.app.ui.components.fragment.promotion_filters.PromotionFiltersBottomFragment"
        android:label="PromotionFiltersBottomFragment">
        <argument
            android:name="selectedFilterId"
            app:argType="long" />
        <argument
            android:name="promotionFilterList"
            app:argType="com.vodovoz.app.ui.model.PromotionFilterUI[]" />
    </dialog>
    <fragment
        android:id="@+id/paginatedProductsCatalogWithoutFiltersFragment"
        android:name="com.vodovoz.app.ui.components.fragment.paginated_products_catalog_without_filters.PaginatedProductsCatalogWithoutFiltersFragment"
        android:label="PaginatedProductsCatalogWithoutFiltersFragment">
        <argument
            android:name="dataSource"
            app:argType="com.vodovoz.app.ui.components.fragment.paginated_products_catalog_without_filters.PaginatedProductsCatalogWithoutFiltersFragment$DataSource" />
        <action
            android:id="@+id/action_to_miniCatalogBottomFragment"
            app:destination="@id/miniCatalogBottomFragment" />
        <action
            android:id="@+id/action_to_sortProductsSettingsBottomFragment"
            app:destination="@id/sortProductsSettingsBottomFragment" />
        <action
            android:id="@+id/action_to_productDetailFragment"
            app:destination="@id/productDetailFragment" />
    </fragment>
    <dialog
        android:id="@+id/miniCatalogBottomFragment"
        android:name="com.vodovoz.app.ui.components.fragment.mini_catalog.MiniCatalogBottomFragment"
        android:label="MiniCatalogBottomFragment" >
        <argument
            android:name="categoryList"
            app:argType="com.vodovoz.app.ui.model.CategoryUI[]" />
        <argument
            android:name="categoryId"
            app:argType="long" />
    </dialog>
    <fragment
        android:id="@+id/allBrandsFragment"
        android:name="com.vodovoz.app.ui.components.fragment.all_brands.AllBrandsFragment"
        android:label="AllBrandsFragment" >
        <argument
            android:name="brandIdList"
            app:argType="long[]"
            app:nullable="true"
            android:defaultValue="@null" />
        <action
            android:id="@+id/action_to_paginatedProductsCatalogWithoutFiltersFragment"
            app:destination="@id/paginatedProductsCatalogWithoutFiltersFragment" />
    </fragment>
    <fragment
        android:id="@+id/productsCatalogFragment"
        android:name="com.vodovoz.app.ui.components.fragment.products_catalog.ProductsCatalogFragment"
        android:label="ProductsCatalogFragment">
        <argument
            android:name="dataSource"
            app:argType="com.vodovoz.app.ui.components.fragment.products_catalog.ProductsCatalogFragment$DataSource" />
        <action
            android:id="@+id/action_to_productDetailFragment"
            app:destination="@id/productDetailFragment" />
    </fragment>
    <dialog
        android:id="@+id/userDataFragment"
        android:name="com.vodovoz.app.ui.components.fragment.user_data.UserDataFragment"
        android:label="UserDataFragment" />
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.vodovoz.app.ui.components.fragment.login.LoginFragment"
        android:label="LoginFragment" >
        <action
            android:id="@+id/action_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.vodovoz.app.ui.components.fragment.register.RegisterFragment"
        android:label="RegisterFragment" />
    <fragment
        android:id="@+id/favoriteFragment"
        android:name="com.vodovoz.app.ui.components.fragment.favorite.FavoriteFragment"
        android:label="FavoriteFragment" >
        <action
            android:id="@+id/action_to_sortProductsSettingsBottomFragment"
            app:destination="@id/sortProductsSettingsBottomFragment" />
        <action
            android:id="@+id/action_to_miniCatalogBottomFragment"
            app:destination="@id/miniCatalogBottomFragment" />
        <action
            android:id="@+id/action_to_productDetailFragment"
            app:destination="@id/productDetailFragment" />
    </fragment>
    <dialog
        android:id="@+id/giftsBottomFragment"
        android:name="com.vodovoz.app.ui.components.fragment.gifts.GiftsBottomFragment"
        android:label="GiftsBottomFragment" >
        <argument
            android:name="giftList"
            app:argType="com.vodovoz.app.ui.model.ProductUI[]" />
    </dialog>
    <dialog
        android:id="@+id/singleRootCatalogBottomFragment"
        android:name="com.vodovoz.app.ui.components.fragment.single_root_catalog.SingleRootCatalogBottomFragment"
        android:label="SingleRootCatalogBottomFragment" >
        <argument
            android:name="categoryId"
            app:argType="long" />
    </dialog>
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.vodovoz.app.ui.components.fragment.profile.ProfileFragment"
        android:label="ProfileFragment" >
        <action
            android:id="@+id/action_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_to_userDataFragment"
            app:destination="@id/userDataFragment" />
        <action
            android:id="@+id/action_profileFragment_to_mapDialogFragment4"
            app:destination="@id/mapDialogFragment4" />
    </fragment>
    <dialog
        android:id="@+id/popupNewsBottomFragment"
        android:name="com.vodovoz.app.ui.components.fragment.popup_news.PopupNewsBottomFragment"
        android:label="PopupNewsBottomFragment" >
        <argument
            android:name="popupNews"
            app:argType="com.vodovoz.app.ui.model.PopupNewsUI" />
    </dialog>
    <dialog
        android:id="@+id/fullScreenHistorySliderFragment"
        android:name="com.vodovoz.app.ui.components.fragment.full_screen_history_slider.FullScreenHistoriesSliderFragment"
        android:label="FullScreenHistorySliderFragment" >
        <argument
            android:name="startHistoryId"
            app:argType="long" />
        <action
            android:id="@+id/action_to_promotionDetailFragment"
            app:destination="@id/promotionDetailFragment" />
        <action
            android:id="@+id/action_to_paginatedProductsCatalogWithoutFiltersFragment"
            app:destination="@id/paginatedProductsCatalogWithoutFiltersFragment" />
        <action
            android:id="@+id/action_to_allBrandsFragment"
            app:destination="@id/allBrandsFragment" />
        <action
            android:id="@+id/action_to_productDetailFragment"
            app:destination="@id/productDetailFragment" />
        <action
            android:id="@+id/action_to_paginatedProductsCatalogFragment"
            app:destination="@id/paginatedProductsCatalogFragment" />
        <action
            android:id="@+id/action_to_productsCatalogFragment"
            app:destination="@id/productsCatalogFragment" />
        <action
            android:id="@+id/action_to_allPromotionsFragment"
            app:destination="@id/allPromotionsFragment" />
    </dialog>
    <dialog
        android:id="@+id/concreteFilterFragment"
        android:name="com.vodovoz.app.ui.components.fragment.concrete_filter.ConcreteFilterFragment"
        android:label="ConcreteFilterFragment" >
        <argument
            android:name="categoryId"
            app:argType="long" />
        <argument
            android:name="filter"
            app:argType="com.vodovoz.app.ui.model.FilterUI" />
    </dialog>
    <dialog
        android:id="@+id/productFiltersFragment"
        android:name="com.vodovoz.app.ui.components.fragment.product_filters.ProductFiltersFragment"
        android:label="ProductFiltersFragment" >
        <argument
            android:name="defaultFiltersBundle"
            app:argType="com.vodovoz.app.ui.model.custom.FiltersBundleUI" />
        <action
            android:id="@+id/action_to_concreteFilterFragment"
            app:destination="@id/concreteFilterFragment" />
        <argument
            android:name="categoryId"
            app:argType="long" />
    </dialog>
    <dialog
        android:id="@+id/fullScreenDetailPicturesSliderFragment"
        android:name="com.vodovoz.app.ui.components.fragment.full_screen_detail_pictures_slider.FullScreenDetailPicturesSliderFragment"
        android:label="FullScreenDetailPicturesSliderFragment" >
        <argument
            android:name="startPosition"
            app:argType="integer" />
        <argument
            android:name="detailPictureList"
            app:argType="string[]" />
    </dialog>
    <dialog
        android:id="@+id/youTubeVideoFragmentDialog"
        android:name="com.vodovoz.app.ui.components.fragment.youtube_player_dialog.YouTubeVideoFragmentDialog"
        android:label="YouTubeVideoFragmentDialog" >
        <argument
            android:name="videoId"
            app:argType="string" />
    </dialog>
    <dialog
        android:id="@+id/allCommentsByProductDialogFragment"
        android:name="com.vodovoz.app.ui.components.fragment.all_comments_by_product.AllCommentsByProductDialogFragment"
        android:label="AllCommentsByProductDialogFragment" >
        <argument
            android:name="productId"
            app:argType="long" />
        <action
            android:id="@+id/action_to_sendCommentAboutProductFragment"
            app:destination="@id/sendCommentAboutProductFragment" />
    </dialog>
    <dialog
        android:id="@+id/sendCommentAboutProductFragment"
        android:name="com.vodovoz.app.ui.components.fragment.send_comment_about_product.SendCommentAboutProductFragment"
        android:label="SendCommentAboutProductFragment" >
        <argument
            android:name="productId"
            app:argType="long" />
    </dialog>
    <dialog
        android:id="@+id/aboutServicesDialogFragment"
        android:name="com.vodovoz.app.ui.components.fragment.about_services.AboutServicesDialogFragment"
        android:label="AboutServicesDialogFragment" />
    <dialog
        android:id="@+id/aboutAppDialogFragment"
        android:name="com.vodovoz.app.ui.components.fragment.about_app.AboutAppDialogFragment"
        android:label="AboutAppDialogFragment" />
    <dialog
        android:id="@+id/mapDialogFragment4"
        android:name="com.vodovoz.app.ui.components.fragment.map.MapDialogFragment"
        android:label="MapDialogFragment" />
</navigation>